<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.password">root</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/hibtest</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <mapping resource="org/cqc/RegisteredManager.hbm.xml" />
        <mapping resource="org/cqc/Chapter.hbm.xml" />
        <mapping resource="org/cqc/JudgementMethod.hbm.xml" />
        <mapping resource="org/cqc/ComplianceCondition.hbm.xml" />
        <mapping resource="org/cqc/RegulatedActivity.hbm.xml" />
        <mapping resource="org/cqc/Provider.hbm.xml" />
        <mapping resource="org/cqc/RoutineCondition.hbm.xml" />
        <mapping resource="org/cqc/NominatedIndividual.hbm.xml" />
        <mapping resource="org/cqc/Outcome.hbm.xml" />
        <mapping resource="org/cqc/ServiceType.hbm.xml" />
        <mapping resource="org/cqc/Location.hbm.xml" />
        <mapping resource="org/cqc/ServiceUserBand.hbm.xml" />
        <mapping resource="org/cqc/Partner.hbm.xml" />
    </session-factory>
</hibernate-configuration>
